{
    "name": "express-ws",
    "version": "1.0.0",
    "description": "tdd with express",
    "main": "App.js",
    "scripts": {
        "start": "cross-env NODE_ENV=development nodemon index",
        "test": "cross-env NODE_ENV=test jest --watch",
        "lint": "eslint . --fix"
    },
    "author": "ssook7979@gmail.com",
    "license": "ISC",
    "dependencies": {
        "@yarnpkg/pnpify": "^4.0.0-rc.36",
        "bcrypt": "^5.1.0",
        "config": "^3.3.8",
        "express": "^4.18.2",
        "express-validator": "^6.14.2",
        "i18next": "^22.4.9",
        "i18next-fs-backend": "^2.1.1",
        "i18next-http-middleware": "^3.2.2",
        "nodemailer": "^6.9.0",
        "sequelize": "^6.28.0",
        "sqlite3": "^5.1.4"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.15",
        "@types/i18next-fs-backend": "^1.1.2",
        "@types/node": "^18.11.18",
        "cross-env": "^7.0.3",
        "eslint": "^8.31.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.3.1",
        "jest-watch-typeahead": "^2.2.1",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.2",
        "smtp-server": "^3.11.0",
        "supertest": "^6.3.3",
        "typescript": "^4.9.4"
    },
    "eslintConfig": {
        "parserOptions": {
            "parser": "babel-eslint",
            "sourceType": "module",
            "allowImportExportEverywhere": true,
            "ecmaVersion": "latest"
        },
        "extends": [
            "eslint:recommended",
            "plugin:prettier/recommended"
        ],
        "plugins": [
            "prettier"
        ],
        "env": {
            "node": true,
            "es6": true,
            "jest": true
        },
        "rules": {
            "prettier/prettier": "warn",
            "eqeqeq": "warn"
        }
    },
    "prettier": {
        "singleQuote": true,
        "printwidth": 120
    },
    "jest": {
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ],
        "verbose": true
    },
    "packageManager": "yarn@3.3.1"
}
